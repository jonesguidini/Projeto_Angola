<div class="card">
    <div class="card-header uppercase">
        <span class="titulo_pagina"><i class="far fa-calendar-alt"></i> @ViewBag.LinkBaseControllerLabel</span>
    </div>

    <div class="card-body">
        <table id="massagens" class="table table-bordered table-hover datatable" cellspacing="0">
            <thead class="thead-dark">
                <tr>
                    <th>Cliente</th>
                    <th>Data</th>
                    <th>Hora</th>
                    <th>Duracao (minutos)</th>
                    <th>Ações</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>

@section scripts{

    <script type="text/javascript" src="~/Scripts/Sistema/Massagens/configDatatable.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            // carrega datatable
            carregaDatatable();
        });
    </script>
}
